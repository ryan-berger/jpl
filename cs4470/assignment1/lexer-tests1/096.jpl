// OK

fn ff(f : float) : float {
   return f * 2.0
}

fn grayscale_pixel({r : float, g : float, b : float, a : float}) : float {
   let r_linear = ff(r)
   let g_linear = ff(g)
   let b_linear = ff(b)
   let y_linear = (0.2126 * r_linear )+ (0.7152 * g_linear) + (0.0722 * b_linear)
   let y = ff(y_linear)
   return y
}

