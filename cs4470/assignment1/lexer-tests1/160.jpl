fn cube1(n : int) : int {
   let a = array[i:n,j:n,k:n] 1
   return sum[i:n,j:n,k:n] a[i,j,k]
}

fn cube2(n : int) : int {
   let x = array[a:n] array[b:n] array[c:n] 1
   return sum[a:n] sum[b:n] sum[c:n] x[a][b][c]
}

fn cube(n : int) : int {
   let a = cube1(n)
   let b = cube2(n)
   assert a == b, "just checking!!!!"
   return a
}

